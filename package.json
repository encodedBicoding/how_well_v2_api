    "@sendgrid/mail": "^7.0.0",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^4.0.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-sslify": "^1.2.0",
    "express-validator": "^6.4.0",
    "fancy-log": "^1.3.3",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.0.0",
